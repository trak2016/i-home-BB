{
    "name": "cake-jcrop/ChartJs",
    "description": "ChartJs plugin for CakePHP 3.0",
    "type": "cakephp-plugin",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ChartJs/Chart.js",
                "version": "1.0.2",
                "dist": {
                    "type": "vcs",
                    "url": "https://github.com/nnnick/Chart.js" 
                } 
            }
        }
    ],
    "require": {
            "php": ">=5.4",
            "cakephp/cakephp": "~3.0",
            "ChartJs/Chart.js": "~1"
        },
    "require-dev": {
            "phpunit/phpunit": "*"
        },
    "autoload": {
            "psr-4": {
                        "ChartJs\\": "src"
                    }
        },
    "autoload-dev": {
            "psr-4": {
                        "ChartJs\\Test\\": "tests",
                        "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
                    }
        }
}
